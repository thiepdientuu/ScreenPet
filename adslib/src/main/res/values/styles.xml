<resources>
    <style name="AdAttribution">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:textColor">@color/textColorPrimary</item>
        <item name="android:textSize">11sp</item>
        <item name="android:gravity">center</item>
        <item name="android:text">@string/ad_attribution</item>
        <item name="android:background">@drawable/bg_text_ad</item>
        <item name="android:paddingStart">4dp</item>
        <item name="android:paddingEnd">4dp</item>
    </style>
    <style name="AdAttributionV2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:textColor">#5369CA</item>
        <item name="android:textSize">11sp</item>
        <item name="android:gravity">center</item>
        <item name="android:text">@string/ad_attribution</item>
        <item name="android:background">@drawable/bg_text_ad_v2</item>
        <item name="android:paddingStart">4dp</item>
        <item name="android:paddingEnd">4dp</item>
    </style>
    <string name="ad_attribution">Ad</string>
</resources>
